generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  fullName  String
  password  String
  phone     String
  role      String?
  image     String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
